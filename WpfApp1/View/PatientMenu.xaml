<Window x:Class="WpfApp1.Service.PatientMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:util="clr-namespace:WpfApp1.View.Util"
        mc:Ignorable="d"
        Title="PatientMenu" Height="768" Width="1024"
        Icon="\Resources\Icons\VeryInterestingLogo.png">
    <Grid>
        <util:TabletMenuBar Grid.Column="1"/>
        <Frame x:Name="PatientFrame" Margin="0, 45, 0, 0" NavigationUIVisibility="Hidden" Background="{StaticResource LightestBlueBrush}"/>
        <Button Style="{StaticResource LogoutButton}" Click="LogoutButton_Click"/>
        <Border x:Name="Overlay" Style="{StaticResource PatientOverlay}"/>
    </Grid>
</Window>
